package org.cake.game.reflect;

/**
 *
 * @author Aaron
 */
public interface Callable<T> {
    
    public final static Object[] EMPTY_ARGS = new Object[0];
    
    public T call(Object[] args);
    
    public T callv(Object... args);
    
    public boolean isValidArgs(Object[] args);
    
}
